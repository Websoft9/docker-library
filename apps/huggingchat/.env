W9_REPO=ghcr.io/huggingface/chat-ui
W9_DIST=community
W9_VERSION=latest

W9_POWER_PASSWORD=1PrMxExC45LsCT

# Environments which for user settings when create application
# Named expression: W9_xxx_xxx_SET, xxx refer to file fields
W9_HTTP_PORT_SET=9001

# Required: OpenAI-compatible API configuration
W9_OPENAI_BASE_URL_SET=https://router.huggingface.co/v1
W9_OPENAI_API_KEY_SET=hf_your_token_here

# Optional: Application name
W9_APP_NAME_SET=HuggingChat

#### --  Not allowed to edit below environments when recreate app based on existing data  -- ####

W9_ID=huggingchat

# W9_HTTP_PORT or W9_HTTPS_PORT is need at leaset and used for proxy for web application
W9_HTTP_PORT=3000

# Container name's suffix must use one of the value
W9_DB_EXPOSE="mongodb"

# It is used when the application APP needs to set an external URL, which can be IP(or domain), IP:PORT
W9_URL=internet_ip:$W9_HTTP_PORT_SET

W9_NETWORK=websoft9

#### ----------------------------------------------------------------------------------------- ####

# Chat UI Configuration - Advanced settings (optional)

# Authentication (OAuth/OpenID)
OPENID_CLIENT_ID=
OPENID_CLIENT_SECRET=
OPENID_SCOPES=openid profile inference-api read-mcp
OPENID_PROVIDER_URL=https://huggingface.co
AUTOMATIC_LOGIN=false

# Model Task (for internal tasks like title generation)
TASK_MODEL=

# LLM Router Configuration (Omni feature)
LLM_ROUTER_ARCH_BASE_URL=
LLM_ROUTER_ARCH_MODEL=
LLM_ROUTER_ROUTES_PATH=
LLM_ROUTER_ENABLE_MULTIMODAL=
LLM_ROUTER_MULTIMODAL_MODEL=
LLM_ROUTER_ENABLE_TOOLS=
LLM_ROUTER_TOOLS_MODEL=
PUBLIC_LLM_ROUTER_DISPLAY_NAME=Omni
PUBLIC_LLM_ROUTER_ALIAS_ID=omni

# MCP (Model Context Protocol) Servers
MCP_SERVERS=
MCP_FORWARD_HF_USER_TOKEN=

# Feature Flags
LLM_SUMMARIZATION=true
ALLOW_IFRAME=true
ENABLE_DATA_EXPORT=true
PUBLIC_SMOOTH_UPDATES=false

# Usage Limits (JSON format)
USAGE_LIMITS={}

# Admin & Metrics
ADMIN_CLI_LOGIN=true
METRICS_ENABLED=false
LOG_LEVEL=info

# Customization
PUBLIC_APP_DESCRIPTION=Making the community's best AI chat models available to everyone.
PUBLIC_GOOGLE_ANALYTICS_ID=
PUBLIC_PLAUSIBLE_SCRIPT_URL=

# Cookie Settings
COOKIE_NAME=hf-chat
COOKIE_SAMESITE=lax
COOKIE_SECURE=false

# HuggingFace Specific
HF_ORG_ADMIN=
HF_ORG_EARLY_ACCESS=
WEBHOOK_URL_REPORT_ASSISTANT=