# Chat UI Configuration - Advanced settings (optional)

# Authentication (OAuth/OpenID)
OPENID_CLIENT_ID=
OPENID_CLIENT_SECRET=
OPENID_SCOPES=openid profile inference-api read-mcp
OPENID_PROVIDER_URL=https://huggingface.co
OPENAI_BASE_URL_SET=https://router.huggingface.co/v1
OPENAI_API_KEY_SET=hf_your_token_here
AUTOMATIC_LOGIN=false
# Model Task (for internal tasks like title generation)
TASK_MODEL=
# LLM Router Configuration (Omni feature)
LLM_ROUTER_ARCH_BASE_URL=
LLM_ROUTER_ARCH_MODEL=
LLM_ROUTER_ROUTES_PATH=
LLM_ROUTER_ENABLE_MULTIMODAL=
LLM_ROUTER_MULTIMODAL_MODEL=
LLM_ROUTER_ENABLE_TOOLS=
LLM_ROUTER_TOOLS_MODEL=
PUBLIC_LLM_ROUTER_DISPLAY_NAME=Omni
PUBLIC_LLM_ROUTER_ALIAS_ID=omni

# MCP (Model Context Protocol) Servers
MCP_SERVERS=
MCP_FORWARD_HF_USER_TOKEN=
# Feature Flags
LLM_SUMMARIZATION=true
ALLOW_IFRAME=true
ENABLE_DATA_EXPORT=true
PUBLIC_SMOOTH_UPDATES=false
# Usage Limits (JSON format)
USAGE_LIMITS={}
# Admin & Metrics
ADMIN_CLI_LOGIN=true
METRICS_ENABLED=false
LOG_LEVEL=info

# Customization
PUBLIC_APP_DESCRIPTION=Making the community's best AI chat models available to everyone.
PUBLIC_GOOGLE_ANALYTICS_ID=
PUBLIC_PLAUSIBLE_SCRIPT_URL=

# Cookie Settings
COOKIE_NAME=hf-chat
COOKIE_SAMESITE=lax
COOKIE_SECURE=false

