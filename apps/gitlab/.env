# Gitlab edition: ce, ee
# how to get Gitlab root password?
# sudo docker exec -it gitlab grep 'Password:' /etc/gitlab/initial_root_password

APP_CE_IMAGEURL=gitlab/gitlab-ce
APP_EE_IMAGEURL=gitlab/gitlab-ee
APP_JH_IMAGEURL=registry.gitlab.cn/omnibus/gitlab-jh
APP_IMAGEURL=$APP_CE_IMAGE
APP_VERSION=latest
APP_NETWORK=websoft9
APP_HTTP_PORT=9001
APP_HTTPS_PORT=443
APP_SSH_PORT=22
APP_NAME=gitlab
APP_USER=root
APP_URL_REPLACE=true
# The URL does not need a protocol, such as http://, just the domain name
APP_URL=appname.example.com
