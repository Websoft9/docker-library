W9_DIST=community
W9_VERSION=latest
W9_REPO=gitea/gitea

#### --  Not allowed to edit below environments when recreate app based on existing data  -- ####
W9_ID=gitea
W9_HTTP_PORT=3000
W9_HTTP_PORT_SET=3001
W9_SSH_PORT_SET=222
W9_URL='InternetIP'
W9_NETWORK=websoft9
#### --------------------------------------------------------------------------------------- ####

# don't need set DOMAIN,ROOT_URL and SSH_DOMAIN because install wizard will get them from user's browser
# install wizard use this SSH_PORT, and container SSH_PORT must the same with it
SSH_PORT=$W9_SSH_PORT_SET
RUN_MODE=prod
DISABLE_REGISTRATION=false
REQUIRE_SIGNIN_VIEW=false