W9_REPO=percona/pmm-server
W9_DIST='community'
W9_VERSION='3.3'
W9_ID='pmm'
W9_HTTPS_PORT=8443
W9_HTTPS_PORT_SET='5466'
W9_URL='appname.example.com'
W9_LOGIN_USER=admin
W9_LOGIN_PASSWORD=admin
W9_NETWORK=websoft9

#----------------- PMM environment if you need ---------------------#
#https://docs.percona.com/percona-monitoring-and-management/setting-up/server/docker.html#environment-variables
#PMM_PUBLIC_ADDRESS=$W9_URL
#DISABLE_UPDATES
#DISABLE_TELEMETRY
#METRICS_RESOLUTION
#METRICS_RESOLUTION_HR
#METRICS_RESOLUTION_MR
#METRICS_RESOLUTION_LR
#DATA_RETENTION
#ENABLE_VM_CACHE
#ENABLE_ALERTING
#ENABLE_AZUREDISCOVER
#ENABLE_BACKUP_MANAGEMENT
#ENABLE_DBAAS
#PMM_DEBUG
#PMM_TRACE
